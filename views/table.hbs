<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Nombre del Producto</th>
      <th scope="col">Precio</th>
      <th scope="col">Imagen</th>
    </tr>
  </thead>
  <tbody>
      {{#each products}}
    <tr>
      <th scope="row">{{inc @index}}</th>
      <td>{{this.title}}</td>
      <td>{{this.price}}</td>
      <td><image src={{{this.image}}} style="height: 4rem;"></image></td>
    </tr>
    {{/each}}
  </tbody>
</table>